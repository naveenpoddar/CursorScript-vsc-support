{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Cursor",
	"patterns": [
		{
			"name": "comment.line.double-slash.cursor",
			"begin": "//",
			"end": "$",
			"beginCaptures": {
				"0": {
					"name": "punctuation.definition.comment.cursor"
				}
			}
		},
		{
			"name": "string.quoted.double.cursor",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.cursor",
					"match": "\\\\."
				}
			]
		},
		{
			"name": "keyword.control.cursor",
			"match": "\\b(fn|let|const|return|if|else|while|for)\\b"
		},
		{
			"name": "entity.name.function.cursor",
			"match": "\\b[a-zA-Z_][a-zA-Z0-9_]*(?=\\s*\\()"
		},
		{
			"name": "variable.other.readwrite.cursor",
			"match": "\\b[a-zA-Z_][a-zA-Z0-9_]*\\b"
		},
		{
			"name": "constant.numeric.cursor",
			"match": "\\b\\d+(\\.\\d+)?\\b"
		},
		{
			"name": "keyword.operator.arithmetic.cursor",
			"match": "(\\+|\\-|\\*|/|=)"
		},
		{
			"name": "punctuation.terminator.cursor",
			"match": ";"
		},
		{
			"name": "punctuation.section.group.cursor",
			"match": "(\\(|\\)|\\{|\\})"
		}
	],
	"scopeName": "source.cursor"
}